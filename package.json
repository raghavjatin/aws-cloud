{
  "name": "aws-cloud",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "start": "npm run serve",
    "build": "npm run lint && npm run build-ts",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --detectOpenHandles --forceExit --verbose --runInBand --coverage",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migrate": "typeorm migration:run",
    "migrate:run": "node_modules/.bin/typeorm migration:run",
    "dev": "pm2-dev server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sgjatin/backend_poc.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sgjatin/backend_poc/issues"
  },
  "homepage": "https://github.com/sgjatin/backend_poc#readme",
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "concurrently": "^7.4.0",
    "eslint": "^8.25.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.186.0",
    "@elastic/elasticsearch": "^8.6.0",
    "aws-sdk": "^2.1231.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "path": "^0.12.7",
    "pg": "^8.5.1",
    "tslib": "^1.11.2",
    "typeorm": "^0.3.10",
    "uuidv4": "^6.2.13"
  },
  "resolutions": {
    "tslib": "1.11.2"
  }
